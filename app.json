    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png"
      },
      "package": "com.luccasalves.WorldCupChampions"
    },
    "web": {
      "favicon": "./assets/icon.png"
    }
  }
